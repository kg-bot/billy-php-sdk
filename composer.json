{
  "name": "lsolesen/billy-php-sdk",
    "type": "library",
    "description": "PHP-SDK to communicate with Billy (http://www.billy.dk)",
    "homepage": "http://github.com/lsolesen/billy-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Lars Olesen",
            "email": "lars@intraface.dk"
        },
        {
          "name": "Matt Glaman",
          "email": "nmd.matt@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Billy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Billy\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=5.3.2",
        "lib/curl": "*"
    },
    "require-dev": {
        "php": ">=5.5.0",
        "phpunit/phpunit": "4.4.*",
        "squizlabs/php_codesniffer": "2.*",
        "satooshi/php-coveralls": "dev-master"
    }
}
